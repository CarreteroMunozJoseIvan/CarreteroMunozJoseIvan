from openpyxl import load_workbook
from pathlib import Path

excel_path = Path(r"C:\RUTA\A\TU\Lineslip_Tracker_2025.xlsx")
wb = load_workbook(excel_path, data_only=True)   # <- lee VALOR
ws = wb["GPF"]

cell = ws["AE190"]  # cambia "AE" por la columna real de Name - Layer
print("data_only=True ->", cell.value)

wb2 = load_workbook(excel_path, data_only=False) # <- lee FÃ“RMULA
ws2 = wb2["GPF"]
cell2 = ws2["AE190"]
print("data_only=False ->", cell2.value)
