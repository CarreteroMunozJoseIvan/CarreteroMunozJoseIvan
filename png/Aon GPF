# Seleccionamos solo las columnas que quieres
df_excel = grouped[["canonical_name", "UMRs"]].copy()

# Convertimos la lista de UMRs a string
df_excel["UMRs"] = df_excel["UMRs"].apply(lambda x: ", ".join(x))

output_path = "accounts_grouped.xlsx"

df_excel.to_excel(
    output_path,
    index=False,
    sheet_name="Accounts"
)
